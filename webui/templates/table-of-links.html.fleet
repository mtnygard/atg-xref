<table cellpadding="0" cellspacing="0" border="0" class="display" id="datatable">
  <thead>
    <tr>
      <th>Name</th>
      <th>References In</th>
      <th>References Out</th>
    </tr>
  </thead>
  <tbody>
  <(map (fn [l] ">
    <tr>
      <td><a href="<(:link l)>"><(:name l)></a></td>
    </tr>
  <") data)>
  </tbody>
  <tfoot>
    <tr>
      <th>Name</th>
      <th>References In</th>
      <th>References Out</th>
    </tr>
  </tfoot>
</table>
